{
  "name": "wn/mate",
  "description": "A tool to generate documentation and tests from PHPDoc comments.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amine Ben hammou",
      "email": "webneat@gmail.com"
    }
  ],
  "bin": ["bin/mate"],
  "autoload": {
    "psr-4": {
      "Wn\\Mate\\": "src/types/",
      "Wn\\Mate\\Classes\\": "src/classes/"
    },
    "files": [
      "src/doc.php",
      "src/test.php",
      "src/parse.php",
      "src/common.php",
      "src/render.php"
    ]
  },
  "require": {
    "php": ">=7.1",
    "tarsana/functional": "^2.2",
    "phpunit/phpunit": "^7.0",
    "tarsana/command": "^1.2"
  }
}
